{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerGroups_samappcontainerinstance_name": {
            "defaultValue": "samappcontainerinstance",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.ContainerInstance/containerGroups",
            "apiVersion": "2024-10-01-preview",
            "name": "[parameters('containerGroups_samappcontainerinstance_name')]",
            "location": "eastus",
            "properties": {
                "sku": "Confidential",
                "confidentialComputeProperties": {
                    "ccePolicy": ""
                },
                "containers": [
                    {
                        "name": "[parameters('containerGroups_samappcontainerinstance_name')]",
                        "properties": {
                            "image": "mcr.microsoft.com/aci/aci-confidential-helloworld:v1",
                            "ports": [
                                {
                                    "protocol": "TCP",
                                    "port": 80
                                }
                            ],
                            "environmentVariables": [],
                            "resources": {
                                "requests": {
                                    "memoryInGB": 1.5,
                                    "cpu": 1
                                }
                            }
                        }
                    }
                ],
                "initContainers": [],
                "restartPolicy": "OnFailure",
                "ipAddress": {
                    "ports": [
                        {
                            "protocol": "TCP",
                            "port": 80
                        }
                    ],
                    "ip": "20.232.71.228",
                    "type": "Public"
                },
                "osType": "Linux"
            }
        }
    ]
}